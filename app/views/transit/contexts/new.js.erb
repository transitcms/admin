<% content = capture do %>
	<%= manage_delivery_for(resource) %>
<% end %>

(function(){
	var count = jQuery('[data-context-id]').length;
	jQuery('#manage_resource_delivery').append("<%= escape_javascript(content) %>");
	jQuery('[data-context-id="<%= resource.id %>"] input[rel=position]').eq(0).val(count + 1);
})();

